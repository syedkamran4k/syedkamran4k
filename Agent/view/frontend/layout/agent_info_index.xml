<?xml version="1.0" encoding="UTF-8"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account" />
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">My Agent</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
<!--            <block class="Magento\Framework\View\Element\Template" name="my_agent_page" template="MyMod_Agent::agent.phtml" />-->
            <block class="MyMod\Agent\Block\Info" name="my_agent_page" template="MyMod_Agent::agent.phtml" />
        </referenceContainer>
    </body>
</page>
